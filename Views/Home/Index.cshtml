@model List<SXJWeb.ViewModels.OrderGroupByUser>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no, minimal-ui" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <title>@ViewData["Title"] - 私享家发货管理系统</title>
    <!-- Path to Framework7 Library CSS-->
    <link rel="stylesheet" href="/css/framework7.ios.min.css" />
    <link rel="stylesheet" href="/css/framework7.ios.colors.min.css" />
    <link rel="stylesheet" href="/css/framework7-icons.css" />
    <!-- Path to your custom app styles-->
    <link rel="stylesheet" href="/css/my-app.css">
</head>
<body>
    <!-- Status bar overlay for fullscreen mode-->
    <div class="statusbar-overlay"></div>
    <!-- Panels overlay-->
    <div class="panel-overlay"></div>
    <!-- Views-->
    <div class="views">
        <!-- 左侧菜单视图-->
        <div class="view view-left navbar-through">
            <div class="navbar">
                <div class="navbar-inner">
                    <div class="center sliding">功能列表</div>
                </div>
            </div>
            <div class="pages">
                <div data-page="index-left" class="page">
                    <div class="page-content">
                        <div class="content-block-title">发货管理</div>
                        <div class="list-block">
                            <ul>
                                <li>
                                    <a href="/home/add" data-view=".view-main" class="item-link">
                                        <div class="item-content">
                                            <div class="item-inner">
                                                <div class="item-title">添加订单</div>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="content-block-title">发货列表</div>
                        <div class="list-block">
                            <ul>
                                <li>
                                    <a href="/home/GroupByUser" data-view=".view-main" class="item-link">
                                        <div class="item-content">
                                            <div class="item-inner">
                                                <div class="item-title">待发订单</div>
                                            </div>
                                        </div>
                                    </a>
                                </li>
                                <!--<li>
                                    <a href="/home/GroupByItem" data-view=".view-main" class="item-link">
                                        <div class="item-content">
                                            <div class="item-inner">
                                                <div class="item-title">待发商品</div>
                                            </div>
                                        </div>
                                    </a>
                                </li>-->
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- 右侧主视图 -->
        <div class="view view-main navbar-through">
            <div class="navbar">
                <div class="navbar-inner">
                    <div class="center sliding">私享家-发货管理系统</div>
                </div>
            </div>
            <!-- Pages-->
            <div class="pages">
                <!-- Page, data-page contains page name-->
                <div data-page="group-by-user" class="page">
                    <!-- Scrollable page content-->
                    <div class="page-content">
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script type="text/template7" id="templateAddItem">
        <div class="card card-small" d-index="{{this.index}}">
            <div class="card-content">
                <img src="{{this.image}}" width="100%" />
                <i class="f7-icons color-red card-close" onclick="deleteCard(this)">close_round_fill</i>
            </div>
            <div class="card-footer card-small-footer">
                <input type="hidden" name="hidFile-{{this.index}}" value="{{this.image}}" />
                <div><input type="text" name="size-{{this.index}}" placeholder="尺码" /></div>
                <div><input type="text" name="supplier-{{this.index}}" placeholder="档口" /></div>
            </div>
        </div>
    </script>
    <!-- Path to Framework7 Library JS-->
    <script type="text/javascript" src="/js/framework7.min.js"></script>
    <!-- Path to your app js-->
    <script type="text/javascript" src="/js/my-app.js"></script>
</body>
</html>